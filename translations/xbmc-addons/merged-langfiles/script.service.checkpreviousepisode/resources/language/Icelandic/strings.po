msgctxt "#32001"
msgid "Missing Episode"
msgstr "Þáttaröð sem vantar"

msgctxt "#32002"
msgid "The previous episode is missing from your library!"
msgstr "Fyrri þáttaröð vantar í safnið þitt!"

msgctxt "#32003"
msgid "Are you sure you want to play this one?"
msgstr "Ertu viss um að þú viljir spila þessa?"

msgctxt "#32004"
msgid "Unwatched Episode"
msgstr "Þáttaröð eftir að horfa á"

msgctxt "#32005"
msgid "You have not watched the previous episode!"
msgstr "Þú hefur ekki horft á fyrri þáttaröð!"

msgctxt "#32006"
msgid "Browse show"
msgstr "Fletta í þáttum"

msgctxt "#32007"
msgid "Would you like to browse the episodes of this show?"
msgstr "Viltu fletta í þáttaröð þessa þáttar?"

msgctxt "#32008"
msgid "Show Option to browse the show"
msgstr "Sýna Valkosti til að fletta í þáttum"
