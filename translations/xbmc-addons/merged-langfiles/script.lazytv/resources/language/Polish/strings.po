msgid "Episode Length (seconds)"
msgstr "Długość Epizodu (w sekundach)"

msgctxt "#32104"
msgid "Rating"
msgstr "Ocena"

msgctxt "#32105"
msgid "Save"
msgstr "Zapisz"

msgctxt "#32106"
msgid "Close"
msgstr "Zamknij"

msgctxt "#32107"
msgid "Exclude All"
msgstr "Wyklucz wszystkie"

msgctxt "#32108"
msgid "Clear"
msgstr "Wyczyść"

msgctxt "#32109"
msgid "Excluded"
msgstr "Wykluczony"

msgctxt "#32110"
msgid "Choose %s to Exclude"
msgstr "Wybierz %s aby wykluczyć"

msgctxt "#32111"
