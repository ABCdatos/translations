msgctxt "#32000"
msgid "General Settings"
msgstr "Allmänna inställningar"

msgctxt "#32001"
msgid "Primary Function"
msgstr "Primär funktion"

msgctxt "#32002"
msgid "Produce a list of 'next episodes' for each TV series"
msgstr "Skala en lista av 'nästa avsnitt' för varje TV-serie"

msgctxt "#32003"
msgid "Create and launch randomised playlist"
msgstr "Skapa och starta slumpad spellista"

msgctxt "#32004"
msgid "Let me choose on launch"
msgstr "Låt mig välja vid start"

msgctxt "#32005"
msgid "Use Smart Playlist to Filter Shows"
msgstr "Använd smarta spellistor för filtrering av program"

msgctxt "#32006"
msgid "Playlist Selection Method"
msgstr "Valmetod för spellista"

msgctxt "#32007"
msgid "Let me choose on launch"
msgstr "Låt mig välja vid start"

msgctxt "#32008"
msgid "Choose a default Playlist"
msgstr "Välj en startspellista"

msgctxt "#32009"
msgid "Select default playlist"
msgstr "Välj en startspellista "

msgctxt "#32010"
msgid "Some shows are fine to watch out of order"
msgstr "En del program går bra att se i slumpvis ordning"

msgctxt "#32011"
msgid "Select random-order shows"
msgstr "Välj program för slumpvis visning"

msgctxt "#32012"
msgid "Only populate with selected TV Shows"
msgstr "Fyll endast med valda TV-program"

msgctxt "#32013"
msgid "Choose shows by ..."
msgstr "Välj program efter ..."

msgctxt "#32014"
msgid "Manual Selection"
msgstr "Manuellt val"

msgctxt "#32015"
msgid "Smart Playlist"
msgstr "Smart spellista"

msgctxt "#32016"
msgid "Click here to select TV Shows"
msgstr "Klicka här för att välja TV-program"

msgctxt "#32030"
msgid "Player Options"
msgstr "Spelaralternativ"

msgctxt "#32031"
msgid "Number of shows in playlist"
msgstr "Antal program i spellista"

msgctxt "#32032"
msgid "Allow multiple episodes of same TV Show"
msgstr "Tillåt multipla avsnitt av samma TV-serie"

msgctxt "#32033"
msgid "Include episodes from unwatched TV series"
msgstr "Inkludera avsnitt från osedda TV-serier"

msgctxt "#32034"
