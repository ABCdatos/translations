msgstr "Starta visade filmer på slumpmässig tid"

msgctxt "#32039"
msgid "Simulates changing the TV channel to a movie in progress"
msgstr "Simulerar kanalbyte till en pågående film"

msgctxt "#32040"
msgid "Movie weight"
msgstr "Vikt för film"

msgctxt "#32041"
msgid "Weight: 1.0 = 1 show, 1 movie, 0.5 = 2 shows, 1 movie"
msgstr "Vikt: 1.0 = 1 program, 1 film, 0.5 = 2 program, 1 film"

msgctxt "#32042"
msgid "Weight: 0.0 = unlimited movies"
msgstr "Vikt: 0.0 = oändligt med film"

msgctxt "#32043"
msgid "Ignore TV shows (movies only)"
msgstr "Ignorera TV-program (endast filmer)"

msgctxt "#32060"
msgid "List Options"
msgstr "Listalternativ"

