msgctxt "#36503"
msgid "Over / Under"
msgstr "Nad sebou"

#. name of a stereoscopic mode
#: system/settings/settings.xml xbmc/video/dialogs/GUIDialogVideoSettings.cpp
msgctxt "#36504"
msgid "Side by side"
msgstr "Vedle sebe"

#. name of a stereoscopic mode
#: system/settings/settings.xml
msgctxt "#36505"
msgid "Anaglyph red / cyan"
msgstr "Anaglyf červená/azurová"

#. name of a stereoscopic mode
#: system/settings/settings.xml
msgctxt "#36506"
msgid "Anaglyph green / magenta"
msgstr "Anaglyf zelená/purpurová"

#. name of a stereoscopic mode
#: system/settings/settings.xml
msgctxt "#36507"
msgid "Interlaced"
msgstr "Prokládané"

#. name of a stereoscopic mode
#: system/settings/settings.xml
msgctxt "#36508"
msgid "Hardware based"
msgstr "Hardwarový"

#. name of a stereoscopic mode
#: system/settings/settings.xml
msgctxt "#36509"
msgid "Monoscopic / 2D"
msgstr "Monoskopické / 2D"

#. name of a stereoscopic mode
#: system/settings/settings.xml
msgctxt "#36510"
msgid "Anaglyph yellow / blue"
msgstr "Anaglyf žlutá/modrá"

#. name of a stereoscopic mode
