msgctxt "Addon Summary"
msgid "Mimic by braz"
msgstr "Mimic by braz"

msgctxt "#31161"
msgid "- Widget icon aspect ratio"
msgstr "- Widget icon aspect ratio"

msgctxt "#31231"
msgid "- Combine submenu and main menu"
msgstr "- Combine submenu and main menu"

msgctxt "#31232"
msgid "Displays the submenu in-line with the main menu. Click once to open the submenu, and again to open the main menu item."
msgstr "Displays the submenu in-line with the main menu. Click once to open the submenu, and again to open the main menu item."

msgctxt "#31235"
msgid "Limits the number of widget items to 20. May improve performance on low-powered devices."
msgstr "Limits the number of widget items to 20. May improve performance on low-powered devices."

msgctxt "#31240"
msgid "- Show icons on main menu"
msgstr "- Show icons on main menu"

msgctxt "#31241"
msgid "Displays icons to the left of the main menu items. Icons must first be selected in the main menu customiser."
msgstr "Displays icons to the left of the main menu items. Icons must first be selected in the main menu customizer."

msgctxt "#31242"
msgid "Select icon"
msgstr "Select icon"

msgctxt "#31243"
msgid "Pick an icon to display on the main menu. Icons will only be displayed on the vertical menu and only if enabled in skin settings."
msgstr "Pick an icon to display on the main menu. Icons will only be displayed on the vertical menu and only if enabled in skin settings."
